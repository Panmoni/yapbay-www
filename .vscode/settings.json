  "editor.tokenColorCustomizations": {
    "comments": "#6272a4",
    "strings": "#f1fa8c",
    "keywords": "#ff79c6",
    "numbers": "#bd93f9",
    "types": "#8be9fd",
    "functions": "#50fa7b",
    "variables": "#f8f8f2",
    "textMateRules": [
      {
        "scope": "entity.name.tag",
        "settings": {
          "foreground": "#ff79c6"
        }
      },
      {
        "scope": "entity.other.attribute-name",
        "settings": {
          "foreground": "#f1fa8c"
        }
      },
      {
        "scope": "markup.heading",
        "settings": {
          "foreground": "#bd93f9",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "markup.italic",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": "markup.bold",
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "scope": "markup.quote",
        "settings": {
          "foreground": "#50fa7b",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "markup.inline.raw",
        "settings": {
          "foreground": "#ffb86c"
        }
      },
      {
        "scope": "markup.fenced_code.block",
        "settings": {
          "foreground": "#6272a4"
        }
      }
    ]
